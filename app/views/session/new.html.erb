<h3>Welcome Back</h3>
<br>
<%= form_tag login_path do %>

    <%= label name="username",  value="Username:" %>
    <%= text_field_tag name="username" %>
    <br>
    <br>
    <%= label name="password", value="Password:" %>
    <%= password_field_tag name="password" %>
    <br>
    <br>
    <%= submit_tag "Log In" %>
  
<% end %>
<br>

<%= form_tag '/auth/github', method: :post do %>
    <%= submit_tag "Log In With GitHub!" %> 
<% end %>